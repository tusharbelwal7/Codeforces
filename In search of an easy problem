#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int t;
    cin>>t;
    vector<int>v;
    for(int i=0;i<t;i++){
        int num;
        cin>>num;
        v.push_back(num);
    }
    int c=0;
    for(int i=0;i<t;i++){
        if(v[i]==1){
            c=1;
            break;
        }
    }
    if(c==1)
        cout<<"HARD";
    else
        cout<<"EASY";

    return 0;
}
