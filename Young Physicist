#include <iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    int x=0,y=0,z=0;
    for(int i=0;i<t;i++){
        int n1,n2,n3;
        cin>>n1>>n2>>n3;
        x+=n1;
        y+=n2;
        z+=n3;
    }
    if(x==0&&y==0&&z==0)
        cout<<"YES";
    else
        cout<<"NO";

    return 0;
}
