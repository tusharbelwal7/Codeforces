#include <iostream>
#include <vector>
using namespace std;


int main()
{
    int t;
    cin>>t;
    vector<char>res;
    for(int i=0;i<t;i++){
        int a,b,c;
        cin>>a>>b>>c;
        if(a+b==c)
            res.push_back('+');
        else if(a-b==c)
            res.push_back('-');
        
    }
    for(auto i:res)
        cout<<i<<endl;

    return 0;
}
