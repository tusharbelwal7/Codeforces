#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int t;
    cin>>t;
    vector<int>res;
    for(int i=0;i<t;i++){
        int a,b;
        cin>>a>>b;
        int r=a%b;
        if(r==0)
            res.push_back(0);
        else{
            int c=b-r;
            res.push_back(c);
        }
    }
    for(int i=0;i<t;i++){
        cout<<res[i]<<endl;
    }

    return 0;
}
