#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int t;
    cin>>t;
    vector<string>res;
    for(int i=0;i<t;i++){
        string s;
        cin>>s;
        int sum1=(int)s[0]+(int)s[1]+(int)s[2];
        int sum2=(int)s[3]+(int)s[4]+(int)s[5];
        if(sum1==sum2)
            res.push_back("YES");
        else
            res.push_back("NO");
    }
    for(int j=0;j<res.size();j++){
        cout<<res[j]<<endl;
    }
    return 0;
}
