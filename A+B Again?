#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int t;
    cin>>t;
    vector<int>res;
    for(int i=0;i<t;i++){
        int num;
        cin>>num;
        int sum=num%10 + num/10;
        res.push_back(sum);
    }
    for(int j=0;j<res.size();j++){
        cout<<res[j]<<endl;
    }
    return 0;
}
