#include <iostream>
using namespace std;
int main()
{
    string s1,s2;
    cin>>s1>>s2;
    int n=s1.length();
    int m=s2.length();
    if(n!=m)
        cout<<"NO";
    else{
        int flag=0;
        for(int i=0;i<n;i++){
            if(s1[i]!=s2[m-1-i]){
                flag=1;
                break;
        }
    }
    if(flag==0)
        cout<<"YES";
    else
        cout<<"NO";
    }
    
    return 0;
}
